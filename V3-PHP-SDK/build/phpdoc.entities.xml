<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>IDG PHP SDK for QuickBooks V3</title>
    <parser>
        <target>output/entities</target>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
            <extension>php3</extension>
            <extension>phtml</extension>
        </extensions>
        <visibility></visibility>
    </parser>
    <transformer>
        <target>../_PhpDocs/entities/</target>
    </transformer>
    <logging>
        <level>emerg</level>
        <paths>
            <default>output/{DATE}.log</default>
            <errors>output/{DATE}.errors.log</errors>
        </paths>
    </logging>
    <transformations>
        <template name="../dependencies/phpdoc/template-ippentitydocs" />
    </transformations>
    <files>
        <directory>../Data/</directory>
    </files>
</phpdoc>